<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>DashBoard</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
      
        <link rel="stylesheet" href="css/const.css">
       
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
   
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-light fixed-top bg-secondary">
            <div class="container">
              <a class="navbar-brand" href="/">Pentos</a>
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
          
           <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
          <li class="nav-item active">
                    <a class="nav-link" href="#">Home</a>
                  </li>
          <li class="nav-item">
                    <a class="nav-link" href="#">About</a>
                  </li>
          <li class="nav-item">
                    <a class="nav-link" href="/proj">Search</a>
                  </li>
          <li class="nav-item">
                    <a class="nav-link" href="/login">Logout</a>
                  </li>
          </ul>
          </div>
          </div>
          </nav>
          
          <div class="container" style="margin-top: 100px;">
            <h1 style="margin-bottom:50px;">Dashboard</h1>

            <div class="row">
              <% if(obj.assigned!=0){ %> 
    
              <div class="card col"style="margin-top: 50px;margin: 5px;">
        <div class="card-body">
          <h5 class="card-title"><%= obj.name %> </h5>
          <h6 class="card-subtitle mb-2 text-muted"><%= obj.email %> </h6>
          
          <% if(obj.project!=" "){ %> 
        
            <h6 class="card-subtitle mb-2 " style="padding-top:5px ;"><%= obj.project %> </h6>
            <h3>Do the project well and earn some Badges :)</h3>
            <h5>  <a href="/photo" class="card-link">Upload todays Photo<a></h5>  
              <% } %> 
          
          <% if(obj.project===" "){ %> 
        
            <p class="card-text"> Join with us and experience a great career ahead for u where your passion and job meets :)</p>
            <% } %> 
            
        
        </div>
      </div>
      <% } %> 
      <% if(obj.assigned===0){ %> 
        <div class="card col"style="margin-top: 50px;margin: 5px;">
          <div class="card-body">
            <h5 class="card-title"><%= obj.name %> </h5>
            <h6 class="card-subtitle mb-2 text-muted"><%= obj.email %> </h6>
            
            <% if(obj.project!=" "){ %> 
          
              <h6 class="card-subtitle mb-2 " style="padding-top:5px ;"><%= obj.project %> </h6>
                <% } %> 
            
            <% if(obj.project===" "){ %> 
          
              <p class="card-text"> Join with us and experience a great career ahead for u where your passion and job meets :)</p>
              <% } %> 
             
              
          
          </div>
        </div>
       
        <% } %> 
    
      <% if(obj.assigned!=0){ %> 
      
      <div class="card col" style="margin-top: 50px;margin: 5px;">
        <div class="card-body">
          <h5 class="card-title">Your Projects :</h5>
          <h4 class="card-text ">  
              <% user.forEach(function(user){ %>
            <% if(user.constr===obj.name){ %>
                <a href="/prog"></a>
                <h4 class="card-subtitle mb-2"><%= user.project %></h4>     
           </a>
                <% } %>  
            <% }) %> 
            </h3>
            
<div class="progress" style="background-color:black;">
    <div class="progress-bar" role="progressbar" style="width: <%= client.progress %>%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%= client.progress %>%</div>
  </div>
          <h5>  <a href="/prog" class="card-link">Todays Progress<a></h5>  
        
            <h5>  <a href="/work" class="card-link">Workers List<a></h5>  
                <h5>  <a href="/cost" class="card-link">Cost<a></h5>  
            
        </div>
      </div>
      <% } %>  
    
      <% if(obj.assigned!=0){ %> 
      <div class="card col" style="margin-top: 50px;margin: 5px;">
        <div class="card-body">
          <h5 class="card-title"><%= client.name %> </h5>
          <h6 class="card-subtitle mb-2 text-muted">Email :<%= client.email %> </h6>
          <h6 class="card-title"><%= client.project %> </h6>
          <h6 class="card-title">Project Description : <%= client.pdesc %> </h6>
          <h6 class="card-title">Estimated Cost : <%= client.cost %> </h6>
        
         <% if(obj.project!=" "){ %> 
        
          <h5>  <a href="/lab" class="card-link">Timeline<a></h5>
                <% } %>  
        
          </div>
      </div>
      <% } %>  
            
    </div>
    <% if(obj.time[0]!=null){ %> 
      <div class="container-fluid" >
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-5">Final Timeline</h4>
        
                        <div class="hori-timeline" dir="ltr">
                        
                          <ul class="list-inline events">
                                <li class="list-inline-item event-list">
                                    <div class="px-4">
                                        <div class="event-date bg-soft-primary text-primary"><%= obj.time[0].phase1 %></div>
                                        <h5 class="font-size-16"> <%= obj.time[0].perc1 %></h5>
                                        <p class="text-muted">Lorem ipsum dolor sit amet, consectetur veniam, quis nostrud exercitation ullamco laboris  </p>
                                        <div>
                                            <a href="#" class="btn btn-primary btn-sm">Read more</a>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-inline-item event-list">
                                    <div class="px-4">
                                        <div class="event-date bg-soft-success text-success"><%= obj.time[0].phase2 %> </div>
                                        <h5 class="font-size-16"><%= obj.time[0].perc2 %></h5>
                                        <p class="text-muted">Lorem ipsum dolor sit amet, consectetur adipiscing elit, veniam, quis nostrud exercitation ullamco laboris </p>
                                        <div>
                                            <a href="#" class="btn btn-primary btn-sm">Read more</a>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-inline-item event-list">
                                    <div class="px-4">
                                        <div class="event-date bg-soft-danger text-danger"><%= obj.time[0].phase3 %></div>
                                        <h5 class="font-size-16"><%= obj.time[0].perc3 %></h5>
                                        <p class="text-muted">If several languages coalesce the grammar of the resulting simple and regular </p>
                                        <div>
                                            <a href="#" class="btn btn-primary btn-sm">Read more</a>
                                        </div>
                                    </div>
                                </li>
                                <li class="list-inline-item event-list">
                                    <div class="px-4">
                                        <div class="event-date bg-soft-primary text-primary"><%= obj.time[0].phase4 %></div>
                                        <h5 class="font-size-16"><%= obj.time[0].perc3 %></h5>
                                        <p class="text-muted">Languages only differ in their pronunciation and their most common words.veniam, quis noshdisa</p>
                                        <div>
                                            <a href="#" class="btn btn-primary btn-sm">Read more</a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- end card -->
            </div>
        </div> 
      <% } %> 
  
</div>
    
       <script src="" async defer></script>
    </body>
</html>