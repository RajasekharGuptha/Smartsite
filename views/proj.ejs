                <h2 class="card-header"><%= user.project %></h2>
                <div class="card-body">
                  <h5 class="card-title"><%= user.typeofwork %> </h5>
                  <p class="card-text"><%= user.pdesc %> </p>
                  <a href="/desc" class="btn btn-primary">Details</a>
                </div>
              </div>
           
          
